{
  "name": "@glrodasz/components",
  "version": "0.4.0",
  "description": "A components library to create apps from zero to production",
  "private": false,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/glrodasz/cero-components.git"
  },
  "engines": {
    "node": ">=15.0.0",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "build:storybook": "build-storybook",
    "build:tokens": "node ./scripts/build-tokens",
    "build": "run-s build:*",
    "deploy:vercel:stag": "vercel",
    "deploy:vercel:prod": "vercel --prod",
    "deploy:stag": "run-s build deploy:vercel:stag",
    "deploy:prod": "run-s build deploy:vercel:prod",
    "dev:storybook": "start-storybook -p 6006",
    "dev:tokens": "watch 'yarn build:tokens' ./tokens",
    "dev": "concurrently 'npm:dev:*'",
    "lint:css:prettier": "prettier '**/*.css' --write",
    "lint:css:stylelint": "stylelint '**/*.css' --fix",
    "lint:css": "yarn lint:css:stylelint && yarn lint:css:prettier",
    "lint:js": "eslint --cache --fix",
    "lint": "run-p lint:*",
    "cc": "node ./scripts/create-component",
    "test": "jest"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Guillermo Rodas <glrodasz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@storybook/addon-actions": "6.0.26",
    "@storybook/addon-essentials": "6.0.26",
    "@storybook/addon-links": "6.0.26",
    "@storybook/addon-storyshots": "6.0.26",
    "@storybook/react": "6.0.26",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "chromatic": "5.2.0",
    "concurrently": "5.3.0",
    "enquirer": "2.3.6",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "minireset.css": "0.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "react": "16.13.1",
    "react-is": "16.13.1",
    "react-test-renderer": "16.13.1",
    "semantic-release": "17.1.2",
    "stylelint": "13.7.2",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-recommended": "3.0.0",
    "watch": "1.0.2"
  },
  "peerDependencies": {
    "react": "16.13.1"
  }
}
